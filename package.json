{
    "name": "my_address_book",
    "version": "1.0.0",
    "description": "This is an address book web app",
    "main": "src/server.js",
    "repository": "https://github.com/jabichris/my_address_book.git",
    "author": "JABIRO Christian",
    "license": "MIT",
    "scripts": {
        "prettier": "prettier --write 'src/**/*.js'",
        "prettier:w": "onchange 'src/**/*.js' -- prettier --write {{changed}}",
        "lint": "eslint src --color",
        "migrate": "npx sequelize-cli db:migrate --config src/config/config.js --migrations-path src/migrations",
        "start": "yarn migrate &&  nodemon -e hbs js src/server.js",
        "test": "jest"
    },
    "dependencies": {
        "@hapi/basic": "^7.0.1",
        "@hapi/hapi": "^21.3.0",
        "@hapi/inert": "^7.0.1",
        "@hapi/vision": "^7.0.1",
        "bcryptjs": "^2.4.3",
        "boom": "^7.3.0",
        "dotenv": "^16.0.3",
        "handlebars": "^4.7.7",
        "hapi": "^18.1.0",
        "hapi-auth-jwt": "^4.0.0",
        "hapi-auth-jwt2": "^10.4.0",
        "inert": "^2.0.0",
        "joi": "^17.7.1",
        "jsonwebtoken": "^9.0.0",
        "mysql2": "^3.1.2",
        "nodemon": "^2.0.20",
        "pg": "^8.9.0",
        "sequelize": "^6.28.0",
        "vision": "^5.4.4"
    },
    "devDependencies": {
        "eslint": "^8.34.0",
        "eslint-config-prettier": "^8.6.0",
        "eslint-config-standard": "^17.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^4.2.1",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-standard": "^5.0.0",
        "jest": "^29.4.3",
        "onchange": "^7.1.0",
        "prettier": "^2.8.4",
        "sequelize-cli": "^6.6.0"
    }
}
